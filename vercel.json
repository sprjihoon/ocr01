{
  "version": 2,
  "functions": {
    "frontend/pages/**/*.tsx": {
      "runtime": "@vercel/node@latest"
    }
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://ocr01-production.up.railway.app"
  }
}